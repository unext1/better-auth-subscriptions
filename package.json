{
  "name": "better-auth-subscriptions",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf node_modules/.vite node_modules/.cache build",
    "build": "react-router build",
    "dev": "react-router dev --host",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc --build --noEmit",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "upg": "pnpm update -s --interactive --latest --ignore-scripts",
    "auth:generate": "better-auth generate --config=app/services/auth.server.ts --output=app/db/schema/index.ts",
    "migrate:gen": "npx drizzle-kit generate",
    "migrate:apply": "npx drizzle-kit migrate",
    "studio": "npx drizzle-kit studio",
    "db:gen": "pnpm tsx app/db/index.ts"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@better-auth/stripe": "^1.4.18",
    "@biomejs/biome": "^2.3.14",
    "@fontsource-variable/dm-sans": "^5.2.8",
    "@libsql/client": "^0.17.0",
    "@phosphor-icons/react": "^2.1.10",
    "@react-router/node": "7.13.0",
    "@react-router/serve": "7.13.0",
    "better-auth": "^1.4.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "input-otp": "^1.4.2",
    "isbot": "^5.1.34",
    "motion": "^12.32.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router": "7.13.0",
    "recharts": "^3.7.0",
    "shadcn": "^3.8.3",
    "sonner": "^2.0.7",
    "stripe": "^20.0.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.4.18",
    "@react-router/dev": "7.13.0",
    "@react-router/remix-routes-option-adapter": "^7.13.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.2.1",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "drizzle-kit": "^0.31.8",
    "remix-flat-routes": "^0.8.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.5"
  }
}
